
# This is an example .env file.
# For local development, create a .env.local file and add your actual credentials there.
# DO NOT COMMIT .env.local or your service account key file to Git.

# Google Sheets API configuration
# Replace with the path to your downloaded service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=./.secure/your-service-account-key.json
# Replace with your actual Google Spreadsheet ID
GOOGLE_SHEET_ID=your_spreadsheet_id_here

# Genkit (if used, for AI features) - these might be set if you configure Genkit
# GOOGLE_API_KEY=your_google_ai_studio_api_key

    